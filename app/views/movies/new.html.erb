

<%= form_with model: @movie, method: :post do |form|%>

<div>
   <%= form.label :name, "Nombre de la Pelicula: "  %>
   <%= form.text_field :name  %>
</div> 

<div>
   <%= form.label :synopsis, "Sinopsis: "  %>
   <%= form.text_field :synopsis  %>
</div> 

<div>
   <%= form.label :director, "Nombre dl director: "  %>
   <%= form.text_field :director  %>
</div> 

   <%=form.submit%>

<% end %>